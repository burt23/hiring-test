{
  "name": "shop-mattress",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "2.1.1"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run start-react\" --kill-others",
    "server": "nodemon NODE_ENV=development server.js",
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write src/**/*.{js,jsx}",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
